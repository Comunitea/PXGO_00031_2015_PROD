                <th>Supplier ref.</th>
                <th>Supplier name</th>
            </xpath>
            <xpath expr="//tbody/t/tr[1]/td[3]" position="after">
                <td></td>
                <td></td>
            </xpath>
            <xpath expr="//tbody/t/tr[2]/td[3]" position="after">
                <td><span t-esc="l['supplier_ref']"/></td>
                <td><span t-esc="l['supplier_name']"/></td>
            </xpath>
        </template>
    </data>
</openerp>
